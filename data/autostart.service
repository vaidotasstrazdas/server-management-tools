[Unit]
Description=Autostart Python Service
After=network.target docker.service

[Service]
Type=simple
ExecStart=/usr/bin/python3 /usr/local/sbin/autostart.pyz
Restart=always
User=bool
WorkingDirectory=/usr/local/sbin/autostart.pyz
Environment=PYTHONUNBUFFERED=1

[Install]
WantedBy=multi-user.target